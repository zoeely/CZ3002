<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ASE_UI">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication2">
        <activity android:name="com.example.ASE_UI.StartGame"></activity>
        <activity android:name="com.example.ASE_UI.HistoryPage" />
        <activity android:name="com.example.ASE_UI.Results_page"
            android:parentActivityName="com.example.ASE_UI.MainMenuPage">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.ASE_UI.MainMenuPage" />  </activity>>
        <activity android:name="com.example.ASE_UI.MainMenuPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>